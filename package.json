{
  "name": "@aasaam/date-time",
  "version": "0.0.6",
  "description": "aasaam date time",
  "main": "lib/AasaamDateTime.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aasaam/date-time.git"
  },
  "keywords": [
    "aasaam",
    "date",
    "gregorian",
    "i18n",
    "islamic",
    "persian",
    "time"
  ],
  "jest": {
