      "lib/**/*.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "json",
      "json-summary",
      "text",
      "lcov",
      "html",
      "clover"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "lodash.groupby": "^4",
    "moment": "^2",
    "moment-hijri": "^2",
    "moment-jalaali": "^0"
  },
  "devDependencies": {
    "@types/jest": "^26",
    "@types/node": "^14",
    "async": "^3",
    "cldr-cal-islamic-full": "^36",
    "cldr-cal-persian-full": "^36",
    "cldr-core": "^36",
    "cldr-dates-full": "^36",
    "country-emoji": "^1",
    "eslint": "^7",
    "eslint-config-airbnb": "^18",
    "eslint-config-prettier": "^6",
    "eslint-plugin-import": "^2",
    "eslint-plugin-sonarjs": "^0",
    "full-icu": "^1",
    "got": "^11",
    "jest": "^26",
    "lodash": "^4",
    "napa": "^3",
    "prettier": "^2"
  },
  "napa-config": {
    "cache": false
  },
  "napa": {
    "cal-events": "persiancal/cal-events#gh-pages"
  },
  "scripts": {
    "lint": "eslint .",
    "napa": "napa",
    "test:cover": "jest --detectOpenHandles --runInBand --collectCoverage",
    "test": "jest --detectOpenHandles --runInBand"
  },
  "authors": [
